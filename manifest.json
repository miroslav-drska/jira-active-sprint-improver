{
  "manifest_version": 3,
  "name": "JIRA Active Sprint Improver",
  "version": "1.0",
  "description": "Enlarges swimlane expander button on JIRA Active Sprint Board.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
